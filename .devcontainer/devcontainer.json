{
    "name": "linkedin_post_scraper",
    "dockerComposeFile": "docker-compose.yml",
    "service": "linkedin_post_scraper",
    "workspaceFolder": "/workspace/",
    "forwardPorts": [
        8888
    ],
    "portsAttributes": {
        "8888": {
            "label": "Jupyter Lab - once started in linkedin_post_scraper container"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "yzhang.markdown-all-in-one",
                "ms-python.python",
                "ms-toolsai.jupyter-keymap",
                "ms-python.autopep8",
                "ms-azuretools.vscode-docker",
                "mohd-akram.vscode-html-format"
            ]
        }
    }
}